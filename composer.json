{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aliziodev/laravel-taxonomy": "^2.6",
        "edulazaro/laratext": "*",
        "eg-mohamed/notable": "^1.4",
        "eg-mohamed/referenceable": "^2.0",
        "genericmilk/docudoodle": "^2.2",
        "iazaran/smart-cache": "^1.3",
        "intervention/image": "^3.11",
        "intervention/validation": "^4.6",
        "jantinnerezo/livewire-alert": "^4.0",
        "jonpurvis/squeaky": "^2.2",
        "laragear/populate": "*",
        "laravel/envoy": "*",
        "laravel/forge-sdk": "*",
        "laravel/framework": "^12.0",
        "laravel/octane": "^2.12",
        "laravel/sanctum": "^4.2",
        "laravel/telescope": "^5.12",
        "laravel/tinker": "^2.10.1",
        "league/geotools": "^1.3",
        "livewire/flux": "^2.4",
        "livewire/livewire": "^3.0",
        "lubusin/laravel-decomposer": "^1.3",
        "mateffy/laravel-introspect": "*",
        "metalogico/laravel-formello": "*",
        "nunomaduro/essentials": "^1.0",
        "omarchouman/lara-util-x": "^1.1",
        "owen-it/laravel-auditing": "^14.0",
        "predis/predis": "^3.2",
        "pusher/pusher-php-server": "^7.2",
        "rappasoft/laravel-livewire-tables": "^3.7",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-livewire-wizard": "^2.4",
        "spatie/laravel-medialibrary": "^11.15",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-tags": "^4.10",
        "usmonaliyev/laravel-simple-rabbitmq": "^1.2",
        "vectorial1024/laravel-cache-evict": "^2.0",
        "wireui/wireui": "^2.4",
        "ziming/laravel-zxcvbn": "^2.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.23",
        "jasonmccreary/laravel-test-assertions": "^2.8",
        "laradumps/laradumps": "^4.5",
        "laravel/boost": "^1.2",
        "laravel/cashier": "^16.0",
        "laravel/dusk": "^8.3",
        "laravel/horizon": "^5.34",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "laravel/scout": "^10.19",
        "laravel/socialite": "^5.23",
        "maatwebsite/excel": "^3.1",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "peckphp/peck": "^0.1.3",
        "phpunit/phpunit": "^11.5.3",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-permission": "^6.21",
        "tyghaykal/laravel-seed-generator": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "format": [
            "php-cs-fixer fix"
        ],
        "format-check": [
            "php-cs-fixer fix --dry-run --diff"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
